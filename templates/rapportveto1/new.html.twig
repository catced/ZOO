{% extends 'base.html.twig' %}

{% block title %}Nouveau Rapport{% endblock %}

{% block body %}
    <h1>Nouveau Rapport</h1>
    {{ form_start(form) }}
    <table>
        <tr>
            <td>{{ form_label(form.animal) }}</td>
            <td>{{ form_widget(form.animal) }}</td>
            <td>{{ form_errors(form.animal) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(form.nourriture) }}</td>
            <td>{{ form_widget(form.nourriture) }}</td>
            <td>{{ form_errors(form.nourriture) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(form.grammage) }}</td>
            <td>{{ form_widget(form.grammage) }}</td>
            <td>{{ form_errors(form.grammage) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(form.etat) }}</td>
            <td>{{ form_widget(form.etat) }}</td>
            <td>{{ form_errors(form.etat) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(form.type) }}</td>
            <td>{{ form_widget(form.type) }}</td>
            <td>{{ form_errors(form.type) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(form.datepassage) }}</td>
            <td>{{ form_widget(form.datepassage) }}</td>
            <td>{{ form_errors(form.datepassage) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(form.detailEtatAnimal) }}</td>
            <td>{{ form_widget(form.detailEtatAnimal) }}</td>
            <td>{{ form_errors(form.detailEtatAnimal) }}</td>
        </tr>
        <tr>
            <td colspan="3">
                <button type="submit">Enregistrer</button>
            </td>
        </tr>
    </table>
    {{ form_end(form) }}
      
    {# {{ form_start(form) }}
        {{ form_row(form.animal) }}     
        {{ form_row(form.nourriture) }}
        {{ form_row(form.grammage) }}
        {{ form_row(form.etat) }}
